.include /home/lab3/ktakagi/spice_model/mos_model3
.include /home/lab3/ktakagi/spice_model/logic.cir 

.options post temp=27

v1 in 0 pwl ( 0.0n 0.0
+ 9.9n 0.0
+ 10.1n 2.5
+ 19.9n 2.5
+ 20.1n 0.0 )

vdd p 0 2.5v

*X1 in out p inv

c1 out 0 30f
*X2 out 0 p inv
*X3 out 0 p inv

.tran 0.01n 30n

.dkfjalsdfmeasure tran t1 trig v(in) val=1.25 td=1ns rise=1 targ v(out) val=1.25 fall=

.control
run
plot v(in) v(out)
*print v(in) v(out)
.endc



.end 
